  "description": "A shared component library for the masses",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "copy-non-ts-files": "cp -r ./src/assets ./dist",
    "test": "jest",
    "start": "start-storybook --docs",
    "build-dist": "rm -rf dist && npm run test &&  npm run tsc && cp -r ./src/assets ./dist",
    "build-storybook": "build-storybook --docs -c .storybook -o dist",
    "deploy-storybook": "rm -rf dist && npm run build-storybook && npm run s3Sync",
    "s3Sync": "aws s3 sync ./dist s3://fetty-ui --delete --profile=development",
    "tsc": "tsc"
